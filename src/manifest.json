{
  "name": "Get Unique CSS Selector",
  "short_name": "Get Selector",
  "description": "Find unique CSS selector of any item on your page. Open context menu and Copy to clipboard",
  "version": "2.1",
  "permissions": ["activeTab", "declarativeContent", "*://*/*", "contextMenus"],
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "32": "icon_32.png",
    "128": "icon_128.png"
  },
  "browser_action": {
    "name": "Click to toggle GetSelector extension"
  },
  "manifest_version": 2
}
